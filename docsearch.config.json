{
    "index_name": "blog",
    "start_urls": [
        "https://blog.jihoon.me/"
    ],
    "sitemap_urls": [
        "https://blog.jihoon.me/sitemap.xml"
    ],
    "sitemap_alternate_links": true,
    "stop_urls": ["/404"],
    "selectors": {
        "lvl0": {
            "selector": "(//header[contains(@id, 'header')] //nav //ul //a /text())[last()]",
            "type": "xpath",
            "global": true,
            "default_value": "Blog"
        },
        "lvl1": "h1",
        "lvl2": "article h2",
        "lvl3": "article h3",
        "lvl4": "article h4",
        "lvl5": "article h5",
        "lvl6": "article h6",
        "text": "article p, article li, article td, article blockquote, ul a"
    },
    "strip_chars": " .,;:#"
}
